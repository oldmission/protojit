// Variant tests

variant Var1 {
  x: int64;
}

variant Var2 {
  y: int64;
}

struct BigStruct {
  _: char8[64];
}

variant Var3 {
  x: int64;
  y: BigStruct;
}

variant Var4 {
  w: char8;
  x: int64;
}

struct Outer {
  v: Var4;
  z: uint8;
}

struct Outer2 {
  z: uint8;
}

struct Outer3 {
  a: Var4;
  b: Var4;
}

struct NestedOuter {
  p: Outer3;
  z: uint8;
}

// TODO: warn that variant has no values and will be generated as an enum.
variant EnumA {
  x;
  y;
  z;
}

variant EnumB {
  z;
  y;
  x;
}

variant NotAnEnum {
  x: uint8;
  y: uint8;
  z: uint8;
}

struct VecVar {
  vec: uint64[:];
  var: Var4;
}

struct Header {
  id: uint64;
}

struct MessageA {
  id: uint64;
  number: uint64;
}

struct MessageB {
  id: uint64;
  character: char8;
}

variant DefaultA {
  default unknown: Header;
  a: MessageA;
}

variant DefaultAB {
  default unknown: Header;
  a: MessageA;
  b: MessageB;
}
